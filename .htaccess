<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.php$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule api/(.*) /wp-content/themes/merepresenta/public/api/$1 [END]
  RewriteRule wp-(.*) wp-$1 [END]
  RewriteRule . /index.php [L]
</IfModule>
